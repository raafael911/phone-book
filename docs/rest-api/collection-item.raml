#%RAML 1.0
usage: This resourceType should be used for any item of a collection of items
  description: A single item of a collection of <<resourcePathName>>
    get:
      description: Get all <<resourcePathName>>, optionally filtered
    put:
      description: Update an existing <<resourcePathName | !singularize>>
    delete:
      description: Delete an existing <<resourcePathName | !singularize>>
